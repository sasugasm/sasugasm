---
layout: head.njk
---

<header>
  <md-tabs>
  <md-primary-tab active aria-label="{{ title }}" aria-controls="{{ title }}-panel">{{ title }}</md-primary-tab>
  {%- for page in collections.pages -%}
    {%- if page.data.title != title -%}
      <md-primary-tab aria-label="{{ page.data.title }}" aria-controls="{{ page.data.title }}-panel"><a href={{ page.data.permalink }}>{{ page.data.title }}</a></md-primary-tab>
    {%- endif -%}
  {%- endfor -%}
  </md-tabs>
</header>
<main>
  <body>
  {{ content | safe }}
  </body>
</main>
